[alias]
    alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\\\t => \\2/' | sort
	br = branch -a
	ci = commit
	cia = commit -a
	co = checkout
	di = diff
	fe = fetch
	me = merge
	st = status
[fetch]
	prune = true
